{
	"parameters": {
		"cvn": "/srv/cvn",
		"common": "/data/project/cvn-common"
	},
	"cwd": "/tmp",
	"user": "cvn.cvnservice",
	"tasks": [
		{
			"cmd": "{cvn}/services/CVNClerkBot/init.sh > {cvn}/log/cvnclerkbot.log 2>&1",
			"match": "python {cvn}/git/CVNClerkBot/cvnclerkbot.py",
			"pool": "cvn-app5",
			"user": false
		}
	],
	"templates": {
		"CVNBot": {
			"cwd": "{cvn}/services/cvnbot/{name}",
			"cmd": "mono {cvn}/services/cvnbot/{name}/CVNBot.exe > {cvn}/log/{name}.log 2>&1",
			"match": "mono {cvn}/services/cvnbot/{name}/CVNBot.exe"
		}
	},
	"template-tasks": {
		"CVNBot": [
			{ "name": "CVNBot1", "pool": "cvn-app5" },
			{ "name": "CVNBot2", "pool": "cvn-app5" },
			{ "name": "CVNBot3", "pool": "cvn-app5" },
			{ "name": "CVNBot4", "pool": "cvn-app4" },
			{ "name": "CVNBot12", "pool": "cvn-app4" },
			{ "name": "CVNBot13", "pool": "cvn-app4" },
			{ "name": "CVNBot14", "pool": "cvn-app5" }
		]
	}
}
